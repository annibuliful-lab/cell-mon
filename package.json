  "license": "MIT",
  "scripts": {
    "backend:serve": "nx serve backend",
    "backend:test": "nx test backend",
    "dev-server:dev": "nodemon -e ts  --exec \"npm run dev-server:start\"",
    "dev-server:start": "UV_THREADPOOL_SIZE=64 && NODE_OPTIONS='--max-old-space-size=8192' ts-node --project tools/tsconfig.tools.json -r tsconfig-paths/register tools/bin/dev-server.ts",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "db-all:generate": "pnpm run db-primary:generate && pnpm run db-job:generate",
    "db-all:migrate": "pnpm run db-primary:migrate && pnpm run db-job:migrate",
    "db-primary:generate": "prisma generate --schema prisma/primary/schema.prisma",
    "db-primary:migrate": "prisma migrate dev --schema prisma/primary/schema.prisma",
    "db-primary:deploy": "prisma migrate deploy --schema prisma/primary/schema.prisma",
    "db-primary:seed": "ts-node prisma/primary/seed",
    "db-job:generate": "prisma generate --schema prisma/job/schema.prisma",
    "db-job:migrate": "prisma migrate dev --schema prisma/job/schema.prisma",
    "db-job:deploy": "prisma migrate deploy --schema prisma/job/schema.prisma",
    "db-job:seed": "ts-node prisma/job/seed",
    "genql": "ts-node --project tools/tsconfig.tools.json -r tsconfig-paths/register tools/bin/genql.ts",
    "codegen:kysely": "kysely-codegen --exclude-pattern _prisma_migrations",
    "codegen:backend": "ts-node --project tools/tsconfig.tools.json -r tsconfig-paths/register tools/bin/codegen.ts",
    "tool:query-builder": "ts-node --project tools/tsconfig.tools.json -r tsconfig-paths/register tools/bin/query-builder.ts"
  },
  "private": true,
